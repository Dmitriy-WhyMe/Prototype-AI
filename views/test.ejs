<!DOCTYPE html>
<html lang="en">
<%- include('blocks/head.ejs') %>
<body>
    <%- include('blocks/menu.ejs') %>
    <h1>Тестирование ИИ:</h1>
    <form method="post" action="/add">
        <div class="row">
            <div class="col s12">
              <div class="row">
                <div class="input-field col s6">
                    <i class="material-icons prefix">textsms</i>
                    <input id="input_placeholder" name="input" type="text" class="validate">
                    <label class="active" for="input_placeholder">input</label>
                </div>
                <div class="input-field col s6">
                    <i class="material-icons prefix">arrow_drop_down</i>
                    <input type="text" id="output_placeholder" name="output" class="autocomplete">
                    <label for="output_placeholder">output</label>
                  </div>
              </div>
            </div>
        </div>
        <button type="submit" class="waves-effect waves-light btn-large">Добавить</button>
    </form>
    <%- include('blocks/scripts.ejs') %>
</body>
</html>